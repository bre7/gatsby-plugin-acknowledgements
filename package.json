{
  "name": "gatsby-plugin-acknowledgements",
  "description": "Creates an Acknowledgements file for each dependency in your package.json",
  "version": "0.0.1",
  "author": "bre7",
  "bugs": {
    "url": "https://github.com/bre7/gatsby-plugin-acknowledgements/issues"
  },
  "dependencies": {
    "@babel/runtime": "^7.7.6",
    "gatsby-core-utils": "^1.0.28",
    "npm-license-crawler": "^0.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.5",
    "@babel/core": "^7.7.5",
    "babel-preset-gatsby-package": "^0.2.16",
    "cross-env": "^5.2.1",
    "jest": "^24.9.0"
  },
  "homepage": "https://github.com/bre7/gatsby-plugin-acknowledgements",
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "favicon",
    "icons",
    "manifest.webmanifest",
    "progressive-web-app",
    "pwa"
  ],
  "license": "MIT",
  "main": "index.js",
  "peerDependencies": {
    "gatsby": "^2.4.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/bre7/gatsby-plugin-acknowledgements.git"
  },
  "scripts": {
    "build": "babel src --out-dir . --ignore **/__tests__",
    "prepare": "cross-env NODE_ENV=production npm run build",
    "watch": "babel -w src --out-dir . --ignore **/__tests__"
  },
  "engines": {
    "yarn": "^1.17.3",
    "node": ">=8.0.0"
  }
}
